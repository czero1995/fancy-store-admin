{
    "name": "vue-base-test",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build"
    },
    "gitHooks": {
        "pre-commit": "lint-staged"
    },
    "lint-staged": {
        "*.{js,less }": [
            "prettier --write"
        ]
    },
    "prettier": {
        "eslintIntegration": true,
        "tabWidth": 2,
        "singleQuote": true,
        "semi": false
    },
    "dependencies": {
        "axios": "^0.19.2",
        "element-ui": "^2.13.2",
        "vue": "^2.6.11",
        "vue-router": "^3.1.6",
        "vuex": "^3.1.3"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "~4.3.0",
        "@vue/cli-plugin-eslint": "~4.3.0",
        "@vue/cli-plugin-router": "~4.3.0",
        "@vue/cli-plugin-vuex": "~4.3.0",
        "@vue/cli-service": "~4.3.0",
        "babel-eslint": "^10.1.0",
        "babel-plugin-component": "^1.1.1",
        "core-js": "^3.6.5",
        "eslint": "^6.7.2",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-vue": "^6.2.2",
        "happypack": "^5.0.1",
        "less": "^3.0.4",
        "less-loader": "^5.0.0",
        "lint-staged": "^10.2.6",
        "prettier": "^2.0.5",
        "vue-template-compiler": "^2.6.11",
        "vuex-persistedstate": "^3.0.1"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended"
        ],
        "parserOptions": {
            "parser": "babel-eslint",
            "ecmaFeatures": {
                "legacyDecorators": true
            }
        },
        "globals": {
            "confirmation": true
        },
        "rules": {
            "vue/require-prop-type-constructor": 0,
            "no-unused-vars": 0
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not dead"
    ]
}
